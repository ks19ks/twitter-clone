<h3>Tweets</h3>
<br>
<table class="table">
  <% @posts.each do |post| %>
  <tr>
    <td><%= post.content %></td>
    <td><%= post.created_at %></td>
    <td><%= link_to 'edit', edit_post_path(post.id) %></td>
    <td><%= link_to 'delete', post_path(post.id), method: :delete, data: { confirm: '本当に削除しますか？' } %></td>
  </tr>
  <% end %>
</table>
<%= link_to 'New Tweet', new_post_path, class: 'btn btn-default' %>
